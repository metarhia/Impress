{
  "extends": "metarhia",
  "globals": {
    "api": false,
    "application": false,
    "connection": false,
    "impress": false
  },
  "plugins": [
    "impress"
  ],
  "rules": {
    "arrow-parens": "off",
    "comma-dangle": "off",
    "handle-callback-err": "off",
    "consistent-return": "off"
  }
}
