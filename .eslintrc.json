{
  "extends": "metarhia",
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "globals": {
    "impress": "readonly",
    "application": "readonly",
    "context": "readonly",
    "api": "readonly",
    "gs": "readonly",
    "Duration": "readonly"
  },
  "plugins": [
    "impress"
  ],
  "rules": {
    "arrow-parens": "off",
    "comma-dangle": "off",
    "handle-callback-err": "off",
    "consistent-return": "off"
  }
}
